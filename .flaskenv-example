FLASK_APP=app
FLASK_DEBUG=true
SQL_USER=sa
SQL_PASS=<SQL_PASSWORD>
SQL_DNS=<SQL_HOST>
SQL_DB_NAME=<SQL_DB_NAME>
SQL_CONN_STRING=DRIVER={ODBC Driver 17 for SQL Server};SERVER=<SQL_HOST>;DATABASE=<SQL_DB_NAME>;UID=<SQL_USERNAME>;PWD=<SQL_PASSWORD>
MONGO_TODO_CONN=mongodb://<MONGO_USERNAME>:<MONGO_PASSWORD>@localhost/<MONGO_DB_NAME>?authSource=admin&tls=false
APP_SECRET_KEY=<VALUE>#any random value can be here
JWT_SECRET_KEY=<VALUE>#any random value can be here
REDIS_URL=redis://localhost:6379